{{> layout/header}}

<div class="container p-5">

    <table class="table table-hover">
        <thead>
        <tr>
            <th>번호</th>
            <th>과정명/회차</th>
            <th>교과목</th>
            <th>시험유형/이유</th>
            <th>학생명</th>
            <th>훈련강사</th>
            <th>점수</th>
            <th>통과여부</th>
            <th>재시험</th>
        </tr>
        </thead>
        <tbody>
        {{#models}}
            <tr>
                <td>{{subjectNo}}</td>
                <td>{{courseNameAndRound}}</td>
                <td>{{subjectTitle}}</td>
                <td>{{paperState}}{{reExamReason}}</td>
                <td>{{studentName}}</td>
                <td>{{teacherName}}</td>
                <td>{{examScore}}</td>
                <td>{{examPassState}}</td>
                <td>
                    {{#isNotPass}}
                        <a class="btn btn-outline-secondary btn-sm" href="#" disabled>재시험응시하기</a>
                    {{/isNotPass}}
                </td>
            </tr>
        {{/models}}
        </tbody>
    </table>

</div>

{{> layout/footer}}