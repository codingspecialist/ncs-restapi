{{> layout/header}}

<div class="container p-5">

    <table class="table table-hover">
        <thead>
        <tr>
            <th>학생명</th>
            <th>교과목</th>
            <th>시험유형/이유</th>
            <th>훈련강사</th>
            <th>점수</th>
            <th>수준</th>
            <th>통과여부</th>
            <th>상세보기</th>
        </tr>
        </thead>
        <tbody>
        {{#models}}
            <tr>
                <td><b>{{studentName}}</b></td>
                <td>{{subjectTitle}}</td>
                <td>{{examState}}{{reExamReason}}</td>
                <td>{{teacherName}}</td>
                <td>{{examScore}}</td>
                <td>{{grade}}</td>
                <td>{{examPassState}}</td>
                <td><a class="btn btn-outline-primary" href="/api/exam/{{examId}}/teacher/result">상세보기</a></td>
            </tr>
        {{/models}}
        </tbody>
    </table>

</div>

{{> layout/footer}}