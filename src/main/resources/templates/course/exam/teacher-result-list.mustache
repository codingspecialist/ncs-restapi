{{> layout/header}}

<div class="container p-5">

    <table class="table table-hover">
        <thead>
        <tr>
            <th>연번</th>
            <th>학생명</th>
            <th>교과목</th>
            <th>시험유형/이유</th>
            <th>훈련강사</th>
            <th>점수</th>
            <th>수준</th>
            <th>통과여부</th>
        </tr>
        </thead>
        <tbody>
        {{#models}}
            <tr class="click-tr" onclick="location.href='/api/teacher/exam/{{examId}}/result?studentNo={{studentNo}}'">
                <td>{{studentNo}}</td>
                <td>{{studentName}}</td>
                <td>{{subjectTitle}}</td>
                <td>{{examState}}{{reExamReason}}</td>
                <td>{{teacherName}}</td>
                <td>{{examScore}}</td>
                <td>{{grade}}</td>
                <td>{{examPassState}}</td>
            </tr>
        {{/models}}
        </tbody>
    </table>

</div>

{{> layout/footer}}