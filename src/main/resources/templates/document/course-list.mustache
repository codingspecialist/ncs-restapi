{{> layout/header}}

<div class="container p-5">

    <table class="table table-hover">
        <thead>
        <tr>
            <th>번호</th>
            <th>과정명/회차</th>
            <th>과정시작일</th>
        </tr>
        </thead>
        <tbody>
        {{#models}}
            <tr class="click-tr" onclick="location.href='/api/teacher/document/course/{{courseId}}/subject'">
                <td>{{courseId}}</td>
                <td>{{courseTitle}}/{{courseRound}}회차</td>
                <td>{{courseStartDate}}</td>
            </tr>
        {{/models}}
        </tbody>
    </table>

</div>

{{> layout/footer}}