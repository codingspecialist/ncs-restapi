{{> layout/header}}

<div class="container p-5">

    <table class="table table-hover">
        <thead>
        <tr>
            <th>번호</th>
            <th>과정명/회차</th>
            <th>과정시작일</th>
        </tr>
        </thead>
        <tbody>
        {{#models}}
            <tr class="click-tr" onclick="location.href='/api/document-menu/course/{{courseId}}/subject'">
                <td>{{courseId}}</td>
                <td>{{courseTitle}}/{{courseRound}}회차</td>
                <td>{{courseStartDate}}</td>
            </tr>
        {{/models}}
        </tbody>
    </table>

    <ul class="pagination d-flex justify-content-center">
        <li class="page-item"><a class="page-link" href="#">Previous</a></li>
        <li class="page-item"><a class="page-link" href="#">Next</a></li>
    </ul>

</div>

{{> layout/footer}}